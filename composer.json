{
    "name": "hasty/filament-environment-indicator",
    "description": "Environment indicator plugin for Laravel Filament.",
    "license": "MIT",
    "keywords": [
        "environment indicator",
        "filament",
        "laravel-filament"
    ],
    "authors": [
        {
            "name": "Igor Lima",
            "email": "me@igorlima.pt"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.0-stable"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Hasty\\FilamentEnvironmentIndicator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Hasty\\FilamentEnvironmentIndicator\\FilamentEnvironmentIndicatorServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    }
}
